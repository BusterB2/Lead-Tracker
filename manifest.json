{
    // Required
    "manifest_version": 3,
    "name": "URL Tracker",
    "version": "1.0.1",
  
    // Recommended
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "description": "A minimalist interface to track URL links while browsing",
  
    // Optional
    "author": "Buster B"
}  
    